<view class="page" bind:__l="__l"><block wx:if="{{logined}}"><view class="new"><view class="sponsor"><view class="new-title">发起人：</view>{{''+sponsor+''}}</view><view class="start"><view class="new-title">起点：</view><view class="innerPlace"><view class="select-place-wrapper"><button data-event-opts="{{[['tap',[['chooseStartPosition']]]]}}" class="select-place-btn" bindtap="__e">选择位置</button></view></view></view><view class="selection"><block wx:if="{{!selectStartPlace}}"><text>尚未选择</text></block><block wx:else><block wx:if="{{selectStartPlace}}"><text>{{'已选:'+selectStartPosition+''}}</text></block></block></view><view class="end"><view class="new-title">终点：</view><view class="innerPlace"><view class="select-place-wrapper"><button data-event-opts="{{[['tap',[['chooseEndPosition']]]]}}" class="select-place-btn" bindtap="__e">选择位置</button></view></view></view><view class="selection"><block wx:if="{{!selectEndPlace}}"><text>尚未选择</text></block><block wx:else><block wx:if="{{selectEndPlace}}"><text>{{'已选:'+selectEndPosition+''}}</text></block></block></view><view class="start-time"><view class="new-title">出发时间：</view><view class="uni-list"><view class="uni-list-cell"><view class="uni-list-cell-db"><picker mode="date" value="{{date}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{date}}</view></picker></view></view></view><view class="uni-list time"><view class="uni-list-cell"><view class="uni-list-cell-db"><picker mode="time" value="{{time}}" start="00:00" end="24:00" data-event-opts="{{[['change',[['bindTimeChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{time}}</view></picker></view></view></view><view class="time-rage"><view class="new-title">时间容错：</view><view class="time-rage-input"><input type="text" data-event-opts="{{[['input',[['__set_model',['$0','time_rage','$event',[]],['']],['input_timeRage',['$event']]]]]}}" value="{{time_rage}}" bindinput="__e"/></view>分钟</view><view class="exist-members"><view class="new-title">已有成员：</view><view class="time-rage-input"><input type="text" data-event-opts="{{[['input',[['__set_model',['$0','exist_members','$event',[]],['']],['input_memberNum',['$event']]]]]}}" value="{{exist_members}}" bindinput="__e"/></view>人</view><view class="exist-members"><view class="new-title">人数限制：</view><view class="time-rage-input"><input type="text" data-event-opts="{{[['input',[['__set_model',['$0','member_limit','$event',[]],['']],['input_memberLimit',['$event']]]]]}}" value="{{member_limit}}" bindinput="__e"/></view>人</view><view class="note"><view class="new-title">备注：</view><view class="note-content"><input type="text" data-event-opts="{{[['input',[['__set_model',['$0','note_content','$event',[]],['']],['input_noteContent',['$event']]]]]}}" value="{{note_content}}" bindinput="__e"/></view></view><view class="createBtn-wrapper"><button class="new-btn" form-type="submit" data-event-opts="{{[['tap',[['crateList']]]]}}" bindtap="__e">创建约单</button><block wx:if="{{created}}"><view class="createListTip">约单创建成功!</view></block></view></view></view></block><block wx:else><block wx:if="{{!logined}}"><view class="nologin_page">还未登录，请先<text data-event-opts="{{[['tap',[['login']]]]}}" class="login_btn" bindtap="__e">登录/注册</text></view></block></block></view>