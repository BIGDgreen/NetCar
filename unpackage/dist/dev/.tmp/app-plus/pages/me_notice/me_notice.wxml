<view class="page me_edit" bind:__l="__l"><uni-list vue-slots="{{['default']}}"><block wx:for="{{notices}}" wx:for-item="notice" wx:for-index="index" wx:key="index"><view class="notice_item"><navigator url="{{'noticeItem/noticeItem?messageId='+notice.id}}"><block wx:if="{{notice.applyType==1}}"><view><uni-list-item title="{{notice.userName+'请求加入您的约单'}}" note="{{'约单号为:'+notice.orderNum+'  申请时间:'+notice.applyTime}}" showArrow="false" show-badge="{{notice.show_notice_badge1}}" badgeText="{{badgeText}}" badgeType="error" data-event-opts="{{[['^click',[['clickNotice1',[index]]]]]}}" bind:click="__e"></uni-list-item></view></block></navigator><navigator url="noticeItem/noticeItem2"><block wx:if="{{notice.applyType==2}}"><view><uni-list-item title="您申请加入的约单没有得到回应" note="{{'约单号为:'+notice.orderNum}}" showArrow="false" show-badge="{{notice.show_notice_badge2}}" badgeText="{{badgeText}}" badgeType="error" data-event-opts="{{[['^click',[['clickNotice2',[index]]]]]}}" bind:click="__e"></uni-list-item></view></block></navigator></view></block><uni-list-item title="欢迎来到carpool!" note="carpool简介" showArrow="false" show-badge="{{show_badge}}" badgeText="{{badgeText}}" badgeType="error" data-event-opts="{{[['^click',[['clickWelcome']]]]}}" bind:click="__e"></uni-list-item></uni-list></view>